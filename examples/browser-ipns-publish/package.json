    "uint8arrays": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@playwright/test": "^1.12.3",
    "delay": "^5.0.0",
    "execa": "^6.0.0",
    "go-ipfs": "0.10.0",
    "ipfsd-ctl": "^10.0.3",
    "parcel": "latest",
    "playwright": "^1.12.3",
