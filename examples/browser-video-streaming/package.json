    "start": "npm run serve",
    "test": "npm run build && playwright test tests --browser=firefox --retries=3"
  },
  "browserslist": "last 1 Chrome version",
  "dependencies": {
    "hls.js": "^0.14.17",
    "hlsjs-ipfs-loader": "^0.3.0",
    "ipfs-core": "^0.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@playwright/test": "^1.12.3",
    "parcel": "latest",
    "playwright": "^1.12.3",
    "rimraf": "^3.0.2",
    "test-util-ipfs-example": "^1.0.2"
  }
}
