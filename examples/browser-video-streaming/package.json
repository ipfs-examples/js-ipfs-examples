{
  "name": "example-browser-video-streaming",
  "version": "1.0.0",
  "private": true,
  "description": "IPFS browser video streaming example",
  "keywords": [],
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./dist ./.cache ./.parcel-cache",
    "build": "parcel build index.html --no-scope-hoist",
